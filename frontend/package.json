{
  "name": "recipe-finder-frontend",
  "version": "0.1.0",
  "main": "index.js",
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run prettier"
    }
  },
  "scripts": {
    "test:watch": "jest --watchAll --setupFilesAfterEnv ./src/__tests__/setup.js",
    "test": "jest --setupFilesAfterEnv ./src/__tests__/setup.js",
    "lint": "eslint . --ext .ts --ext .tsx",
    "lint:fix": "eslint . --ext .ts --ext .tsx --fix",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "start": "webpack serve",
    "build": "webpack --mode production"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.0.0",
    "css-loader": "^6.7.3",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.3",
    "jest": "^29.4.3",
    "jsdom": "^21.1.0",
    "prettier": "^2.8.4",
    "sass": "^1.58.2",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "typescript": "~4.5.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  }
}
